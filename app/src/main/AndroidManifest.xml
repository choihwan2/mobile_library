<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fourB.library">
    <uses-permission android:name="android.permission.INTERNET"/>

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Guide_Ipad"
            android:label="@string/guide_ipad_page_name"></activity>
        <activity android:name=".Guide_Floor"
            android:label="@string/guide_floor_page_name"/>
        <activity android:name=".Guide_Borrow"
            android:label="@string/guide_borrow_page_name"/>

        <activity
            android:name=".Login"
            android:theme="@style/Login_Theme"
            android:label="@string/login_page_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StudyRoomRVActivity"
            android:label="@string/studyroom_page_name"/>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ChatBotActivity"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".StudyRoom"
            android:theme="@style/AppTheme"
            android:label="@string/studyroom_page_name"/>


        <activity
            android:name=".EbookActivity"
            android:theme="@style/AppTheme"
            android:configChanges="orientation|keyboardHidden|screenSize">
        </activity>

        <activity
            android:name=".ReadingRoom.ReadingRoomActivity"
            android:theme="@style/AppTheme" />

    </application>

</manifest>